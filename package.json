{
  "name": "yalertui",
  "version": "0.0.0",
  "private": false,
  "scripts": {
    "dev:lib": "lerna run --scope components dev --stream",
    "dev:docs": "lerna run --scope docs dev --stream",
    "start": "lerna run dev --scope docs --stream",
    "dev": "node scripts/dev.js",
    "build": "node scripts/build.js",
    "clean": "lerna clean",
    "docs:dev": "pnpm run -C packages/valaxy-docs dev",
    "docs:build": "pnpm run -C packages/valaxy-docs build",
    "dev:play": "pnpm -C packages/play dev",
    "test:lib": "pnpm -C packages/yAlterUI test",
    "test:dev": "pnpm -C packages/yAlterUI test:dev",
    "test:cover": "pnpm -C packages/yAlterUI test:cover",
    "build:lib": "pnpm -C packages/yAlterUI build",
    "build:docs": "lerna run build --scope docs --stream",
    "build:utils": "pnpm -C packages/common/utils build && pnpm i",
    "build:hooks": "pnpm -C packages/common/hooks build && pnpm i",
    "build:icons": "pnpm -C packages/common/icons build && pnpm i",
    "publish:independent": "node scripts/publish.js",
    "publish": "lerna publish"
  },
  "workspaces": [
    "packages/*"
  ],
  "dependencies": {
    "vue": "^3.2.47",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^4.0.3",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/prismjs": "^1.26.0",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "acorn-walk": "^8.2.0",
    "autoprefixer": "^10.4.14",
    "cross-env": "^7.0.3",
    "cross-spawn": "^7.0.3",
    "cssnano": "^6.0.0",
    "execa": "^7.1.1",
    "filter": "^0.1.1",
    "front-matter": "^4.0.2",
    "fs-extra": "^11.1.1",
    "globby": "^13.1.3",
    "handlebars": "^4.7.7",
    "inquirer": "^9.1.5",
    "lerna": "^6.6.1",
    "node-fetch": "^3.3.1",
    "postcss": "^8.4.21",
    "prismjs": "^1.29.0",
    "rimraf": "^4.4.1",
    "rollup": "^3.20.2",
    "rollup-plugin-dts": "^5.3.0",
    "rollup-plugin-sass": "^1.12.19",
    "sass": "^1.60.0",
    "tslib": "^2.5.0",
    "typescript": "^5.0.3",
    "vite": "^4.3.1",
    "vite-plugin-vue-devtools": "^0.0.3",
    "vitest": "^0.30.1",
    "vue-tsc": "^1.2.0"
  }
}
