{
  "name": "yalertui",
  "version": "0.0.0",
  "private": false,
  "scripts": {
    "dev:lib": "lerna run --scope components dev --stream",
    "dev:docs": "lerna run --scope docs dev --stream",
    "start": "lerna run dev --scope docs --stream",
    "dev": "node scripts/dev.js",
    "build": "node scripts/build.js",
    "clean": "lerna clean",
    "docs:dev": "pnpm run -C packages/docs dev",
    "dev:play": "pnpm -C packages/play dev",
    "build:lib": "lerna run build --scope components --stream",
    "build:docs": "lerna run build --scope docs --stream",
    "build:utils": "pnpm -C packages/common/utils build && pnpm i",
    "build:hooks": "pnpm -C packages/common/hooks build && pnpm i",
    "build:icons": "pnpm -C packages/common/icons build && pnpm i",
    "publish:independent": "node scripts/publish.js",
    "publish": "lerna publish"
  },
  "workspaces": [
    "packages/*"
  ],
  "dependencies": {
    "play": "^0.5.0",
    "vue": "^3.2.25",
    "vue-router": "^4.0.15"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/preset-typescript": "^7.16.7",
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^8.3.2",
    "@types/prismjs": "^1.26.0",
    "@vitejs/plugin-vue": "^2.0.0",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "@vue/babel-plugin-jsx": "^1.1.1",
    "acorn-walk": "^8.2.0",
    "autoprefixer": "^10.4.9",
    "cross-env": "^7.0.3",
    "cross-spawn": "^7.0.3",
    "cssnano": "^5.1.13",
    "execa": "^6.1.0",
    "filter": "^0.1.1",
    "front-matter": "^4.0.2",
    "fs-extra": "^10.0.0",
    "globby": "^13.1.2",
    "handlebars": "^4.7.7",
    "happy-dom": "^6.0.4",
    "inquirer": "^8.2.0",
    "lerna": "^5.2.0",
    "node-fetch": "^3.2.10",
    "postcss": "^8.4.16",
    "prismjs": "^1.25.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.77.2",
    "rollup-plugin-dts": "^4.2.2",
    "rollup-plugin-sass": "^1.2.13",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.32.1",
    "sass": "^1.52.3",
    "tslib": "^2.4.0",
    "typescript": "^4.4.4",
    "vite": "^2.7.2",
    "vitest": "^0.21.1",
    "vue-tsc": "^0.29.8"
  }
}
