{
  "name": "yalertui",
  "version": "0.0.0",
  "private": false,
  "scripts": {
    "dev:lib": "lerna run --scope components dev --stream",
    "dev:docs": "lerna run --scope docs dev --stream",
    "start": "lerna run dev --scope docs --stream",
    "dev": "node scripts/dev.js",
    "build": "node scripts/build.js",
    "clean": "lerna clean",
    "docs:dev": "pnpm run -C packages/valaxy-docs dev",
    "docs:build": "pnpm run -C packages/valaxy-docs build",
    "dev:play": "pnpm -C packages/play dev",
    "test:lib": "pnpm -C packages/yAlterUI test",
    "test:dev": "pnpm -C packages/yAlterUI test:dev",
    "test:cover": "pnpm -C packages/yAlterUI test:cover",
    "build:lib": "pnpm -C packages/yAlterUI build",
    "build:docs": "lerna run build --scope docs --stream",
    "build:utils": "pnpm -C packages/common/utils build && pnpm i",
    "build:hooks": "pnpm -C packages/common/hooks build && pnpm i",
    "build:icons": "pnpm -C packages/common/icons build && pnpm i",
    "publish:independent": "node scripts/publish.js",
    "publish": "lerna publish"
  },
  "workspaces": [
    "packages/*"
  ],
  "dependencies": {
    "vue": "^3.3.1",
    "vue-router": "^4.2.0"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^5.0.0",
    "@rollup/plugin-commonjs": "^24.1.0",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@rollup/plugin-typescript": "^11.1.0",
    "@types/prismjs": "^1.26.0",
    "@vitejs/plugin-vue": "^4.2.2",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "acorn-walk": "^8.2.0",
    "autoprefixer": "^10.4.14",
    "cross-env": "^7.0.3",
    "cross-spawn": "^7.0.3",
    "cssnano": "^6.0.1",
    "execa": "^7.1.1",
    "filter": "^0.1.1",
    "front-matter": "^4.0.2",
    "fs-extra": "^11.1.1",
    "globby": "^13.1.4",
    "handlebars": "^4.7.7",
    "inquirer": "^9.2.2",
    "lerna": "^6.6.2",
    "node-fetch": "^3.3.1",
    "postcss": "^8.4.23",
    "prismjs": "^1.29.0",
    "rimraf": "^5.0.0",
    "rollup": "^3.21.6",
    "rollup-plugin-dts": "^5.3.0",
    "rollup-plugin-sass": "^1.12.19",
    "sass": "^1.62.1",
    "tslib": "^2.5.0",
    "typescript": "^5.0.4",
    "vite": "^4.3.5",
    "vite-plugin-vue-devtools": "^0.0.15",
    "vitest": "^0.31.0",
    "vue-tsc": "^1.6.4"
  }
}
