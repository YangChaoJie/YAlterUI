{
  "name": "yalert-ui",
  "version": "0.0.1",
  "author": {
    "name": "jerry yang",
    "email": "linghulingxi@gmail.com"
  },
  "private": false,
  "description": "all components are settled here",
  "license": "MIT",
  "main": "lib/components/index.mjs",
  "module": "lib/components/index.mjs",
  "peerDependencies": {
    "vue": "^3.2.25"
  },
  "scripts": {
    "dev": "vite --host --mode development --config ./build/dev.config.ts",
    "test": "vitest --config ./build/vitest.config.ts ",
    "build:l": "vue-tsc --noEmit --skipLibCheck && vite build --mode production --config ./build/prod.config.ts",
    "build": "rimraf lib dist && rollup --config build/rollup.config.js && yarn build:lib && yarn build:types",
    "build:lib": "cross-env NODE_ENV=lib babel src --out-dir lib --source-maps --extensions \".ts\",\".tsx\",\".snap\" --copy-files --no-copy-ignored --out-file-extension .mjs",
    "build:types": "rimraf types-temp && tsc --pretty --emitDeclarationOnly -p tsconfig.json && rollup --config build/rollup.types.config.js && rimraf types-temp",
    "preview": "vite preview"
  },
  "exports": {
    ".": {
      "import": "./lib/components/index.mjs",
      "require": "./dist/yalterui.js"
    },
    "./components": "./src/components/index.ts",
    "./components/*": "./lib/components/*/index.mjs"
  },
  "types": "lib/index.d.ts",
  "styles": "lib/styles/main.css",
  "typesVersions": {
    "*": {
      "lib/framework.mjs": [
        "lib/index.d.ts"
      ],
      "framework": [
        "lib/index.d.ts"
      ],
      "lib": [
        "lib/index.d.ts"
      ],
      "*": [
        "*",
        "dist/*",
        "lib/*",
        "lib/*.d.ts",
        "lib/*/index.d.ts"
      ]
    }
  },
  "files": [
    "dist/",
    "lib/"
  ],
  "devDependencies": {
    "@babel/preset-env": "^7.17.10",
    "@babel/preset-typescript": "^7.18.6",
    "@rollup/plugin-alias": "^3.1.9",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^8.3.2",
    "@types/prismjs": "^1.26.0",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "@vue/babel-plugin-jsx": "^1.1.1",
    "@vue/compiler-sfc": "^3.0.5",
    "@vue/test-utils": "^2.0.0-rc.18",
    "acorn-walk": "^8.2.0",
    "autoprefixer": "^10.4.9",
    "babel-plugin-add-import-extension": "1.5.1",
    "babel-plugin-component": "^1.1.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "cross-env": "^7.0.3",
    "cssnano": "^5.1.13",
    "filter": "^0.1.1",
    "front-matter": "^4.0.2",
    "happy-dom": "^6.0.4",
    "postcss": "^8.4.16",
    "prismjs": "^1.25.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.77.2",
    "rollup-plugin-dts": "^4.2.2",
    "rollup-plugin-sass": "^1.2.13",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.32.1",
    "tailwindcss": "^2.2.19",
    "tslib": "^2.4.0",
    "typescript": "^4.4.4",
    "unplugin-vue-components": "^0.22.7",
    "unplugin-vue-define-options": "^0.6.1",
    "vite": "^3.0.2",
    "vite-plugin-dts": "^0.9.6",
    "vitest": "^0.21.1",
    "vue-tsc": "^0.34.7"
  },
  "dependencies": {
    "@vue/shared": "^3.2.40",
    "@yalert-ui/hooks": "*",
    "@yalert-ui/icons": "0.0.1",
    "play": "^0.5.0",
    "vue-router": "^4.1.3"
  }
}
